{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Deny",
            "Action": "*",
            "Resource": "*",
            "Condition": {
                "ArnLike": {
                    "aws:PrincipalArn": [
                        "arn:aws:iam::*:root"
                    ]
                },
                "Null": {
                    "aws:AssumedRoot": "true"
                }
            }
        }
    ]
}